{
    "name": "Custom Cascades",
    "description": "Add custom CSS to any web page.",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "background",
        "*://*/*"
    ],
    "options_page": "options.html",
    "background": {
        "scripts": ["background.js","md5.min.js"],
        "persistent": false
    }
}
